{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileCleanup": {
    "TempFilesPath": "D:\\fileuploader_tempfiles",
    "RetentionHours": 2
  },

  "ConnectionStrings": {
    "Redis": "srvredis.local:6379"
  },
  "S3": {
    "BucketName": "",
    "Region": "",
    "AccessKey": "",
    "SecretKey": "",
    "BaseUrl": ""
  },
  "FileUpload": {
    "MaxFileSizeInBytes": 5242880,
    "TempFilesPath": "temp",
    "InitialImageQuality": 90,
    "MinImageQuality": 10,
    "QualityReductionStep": 10
  },
  "MongoDB": {
    "ConnectionString": "",
    "DatabaseName": "filemanager",
    "FilesToRemoveCollection": "filesToRemove"
  }
}
